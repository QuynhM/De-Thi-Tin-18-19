def beauty(n):
  def is_prime(num):
    if num == 0: return False
    if num == 1: return False
    i=2
    while i*i<=num:
      if num % i == 0:
          return False
      i+=1
    return True
  
  count = 0
  for i in range(11,100000):
    sum_s = 0
    for s in str(i):
      sum_s += int(s)**2
    if is_prime(sum_s):
      count+=1 
    if count == n:
      return i

